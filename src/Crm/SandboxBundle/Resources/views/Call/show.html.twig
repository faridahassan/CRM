{% extends '::base.html.twig' %}

{% block page_content -%}
    <h1>Call</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Outcome</th>
                <td>{{ entity.outcome }}</td>
            </tr>
            <tr>
                <th>Objective</th>
                <td>{{ entity.objective }}</td>
            </tr>
            <tr>
                <th>Orientation</th>
                <td>{{ entity.orientation }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('call') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('call_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
